
@{
    Layout = "~/Views/Shared/_Layout.cshtml";
}





<br />
<div class="row">
    <div class="col-6">
        <h2 class="text-primary">Vendor Contact </h2>
    </div>
    <div class="col-6 text-right">
        <a class="btn btn-primary" asp-action="Upsert"><i class="fas fa-plus"></i> &nbsp; Create Vendor Contact</a>
    </div>
</div>

<br />
<div class="p-4 border rounded">
    <table id="tblData" class="table table-striped table-bordered" style="width:100%">
        <thead class="thead-dark">
            <tr class="table-info">
                @*
        
                *@
                <th>Contact Name</th>
                <th>Contact Email</th>
                <th>Contact Phone</th>
                <th>Contact Branch</th>
                <th>Contact Remark</th>
                <th>Vendor</th>



                <th></th>
            </tr>
        </thead>
    </table>
</div>




@section Scripts {

    <script src="~/js/vendorcontact.js"></script>

}

